---
layout: nil
---
<?xml version="1.0"?>
<feed xmlns="http://www.w3.org/2005/Atom">
	<title>Cocoahero</title>
	<link href="http://cocoahero.com/"/>
	<link type="application/atom+xml" rel="self" href="http://cocoahero.com/feed/atom.xml"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://cocoahero.com/</id>
	<author>
		<name>Jonathan Baker</name>
	</author>
	{% for post in site.posts %}
	<entry>
		<id>{{ post.url }}</id>
    	<link type="text/html" rel="alternate" href="{{ post.url }}"/>
    	<title>{{ post.title }}</title>
    	<updated>{{ post.date | date_to_xmlschema }}</updated>
    	<author>
      		<name>Jonathan Baker</name>
      		<uri>http://cocoahero.com/</uri>
    	</author>
    	<content type="html">{{ post.content | xml_escape }}</content>
  	</entry>
  	{% endfor %}
</feed>